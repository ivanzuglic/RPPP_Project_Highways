@model IEnumerable<RPPP12.Models.Stanje>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VrijemePocetka)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VrijemeZavrsetka)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Opis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SifraDogadajNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SifraZabranaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VrijemePocetka)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VrijemeZavrsetka)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Opis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SifraDogadajNavigation.SifraDogadaj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SifraZabranaNavigation.SifraZabrana)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SifraStanje">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SifraStanje">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SifraStanje">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
