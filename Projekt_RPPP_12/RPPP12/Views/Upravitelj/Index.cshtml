@model IEnumerable<RPPP12.Models.Upravitelj>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Oib)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SifraSjedistaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Oib)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SifraSjedistaNavigation.Adresa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SifraUpravitelja">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SifraUpravitelja">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SifraUpravitelja">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
